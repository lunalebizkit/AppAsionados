<div id="cancha">
    <div class="d-flex justify-content-around">
        <div class="container-fluid">
            <div class="row justify-content-between">
                <div id="tablaEquipo" class="col-md-9 m-auto p-3">
                    <form class="d-flex">
                        
                        <h2 id="titDuenio" class= "md-3 px-5"><b>CANCHAS</b></h2>
                        
                        
                        <div class="d-flex justify-content-between m-auto ">
                            <div class="btn-group" role="group" aria-label="Basic outlined example">
                                <a type="button" class="btn btn-primary" href="establecimiento">Agregar Establecimiento</a>
                            </div>
                            <div class="btn-group" role="group" aria-label="Basic outlined example">
                                <a type="button" class="btn btn-success" href="cancha">Agregar Cancha</a>
                            </div>
                        </div>
                        
                    </form>
                    <div id="scrollDuenio">
                        <table id="tablaScroll" class="table table-hover table-info">
                            <thead>
                                <tr>
                                    <th>Nombre</th>
                                    <th>Dirección</th>
                                    <th>Formato</th>
                                    <th>Número</th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each equipos}}
                                <tr>
                                    <td data-bs-toggle="tooltip" data-bs-placement="top" title="{{email}}">{{nombreUsuario}}
                                    </td>
                                    <td><a href="/paginas/equipo/{{nombreEquipo}}">{{nombreEquipo}}</a></td>
                                    <td>{{deporte}}</td>
                                </tr>
                                {{/each}}
                            </tbody>
                        </table>
                    </div>
                </div>
               
            </div>
        </div>
    </div>
    <div class="col-2  p-3">
        <a href="/paginas/inicio" type="button" class="btn btn-primary">Volver</a>
    </div>
    
</div>