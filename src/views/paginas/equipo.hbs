<div class="container">
    <table class="table table-dark table-striped">
        <thead>
            <h2 class="text-center" style="color: aquamarine;">{{club}}</h2>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Posicion</th>
        </thead>
        <tbody>
            {{#each equipo}}
            <tr>
                <td>{{nombre}}</td>
                <td>{{apellido}}</td>
                <td>{{posicion}}</td>                
            </tr>
                {{/each}}
            
        </tbody>
    </table>
    <div class="col-2  p-3">
        <a href="/paginas/futbol/{{user.idUsuarios}}" type="button" class="btn btn-dark">Volver</a>
    </div>
</div>