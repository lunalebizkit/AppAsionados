<div id="inicio" class="container-fluid">
    <div class="col-md-9 m-auto p-4">
        <h3><p class=" card text-center py-2" style="color: rgb(0, 0, 0);">{{equipo}}</p></h3>
        <div id="scrollMiEquipo">
            <table class="table table-hover table-dark">
                <thead class="table-info">
                    <th>Usuario</th>
                    <th>Jugador</th>
                    <th>Posicion</th>
                </thead>
                <tbody>            
                    {{#each equipos}}
                    <tr>
                    <td>{{nombreUsuario}}</td>
                    <td>{{nombre}}</td>
                    <td>{{posicion}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
    <div class="col-2  p-4">
        <a id="volver" href="javascript: history.go(-1)" type="button" class="btn btn-danger">Volver</a>
    </div>
</div>