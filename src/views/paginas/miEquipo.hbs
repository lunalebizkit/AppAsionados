<div id="formularioF" class="container">
    <div id="scrollMiEquipo" class="col-md-9 m-auto p-4">
        <table class="table table-dark">
            <h3><p id="semiTransparente" class="text-center py-2" style="color: rgb(0, 0, 0);">{{equipo}}</p></h3>
            <thead class="table-danger">
                <th>Usuario</th>
                <th>Jugador</th>
                <th>Posicion</th>
            </thead>
            <tbody>            
                {{#each equipos}}
                <tr>
                <td>{{nombreUsuario}}</td>
                <td>{{nombre}}</td>
                <td>{{posicion}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
    <div class="col-2  p-4">
        <a id="volver" href="javascript: history.go(-1)" type="button" class="btn btn-danger">Volver</a>
    </div>
</div>