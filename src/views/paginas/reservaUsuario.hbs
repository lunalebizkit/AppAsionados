     var tiempoActual= new Date().getHours();
    var dia= new Date().getDay();
    var horaReserva= document.getElementById('24').innerText
    var hora = horaReserva.split(/[/ :]/)[0]-15;
     var fecha= new Date(document.getElementById('10').innerHTML).getUTCDay();
