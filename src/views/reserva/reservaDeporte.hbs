<div id="ticket">
    <div class="progress">
        <div class="progress-bar progress-bar-striped bg-success progress-bar-animated" role="progressbar"
            aria-valuenow="25" aria-valuemin="0" aria-valuemax="100" style="width: 25%"></div>
    </div>
    <div class="container-fluid py-2">
        <div class="row row-cols-2 pt-5 pb-5">
            <div class="col-md-4 py-5" style="background-color: rgb(255, 255, 255, 0.5);">
                <h3 class="text-center mb-4">Elige un Deporte</h3>
                <div class="text-center">
                     {{#each deporte}}
                    <a href="/paginas/reservaDeporte1/{{idDeportes}}" class="btn btn-primary mb-3" name="" >{{deporte}}</a>
                    {{/each}}
                </div>
                <div class="row pt-5">
                    <div class="container m-0 pt-5">
                        <a id="volver" href="/paginas/inicio" type="button" class="btn btn-danger">Volver</a>
                    </div>
                </div>

            </div>
            <div class="col">
                  <div id="btnCentro" class="mx-auto">
                <!-- logo de la app -->
                    <img src="/img/app/logo4.png" alt="Logo App" class="mx-1">
                </div>
            </div>
        </div>
    </div>

</div>